{
  "name": "api-example",
  "version": "0.0.1",
  "description": "Example project built with sdkgen and TypeORM",
  "scripts": {
    "dev": "nodemon --exec node --async-stack-traces --inspect -r ts-node/register -r dotenv/config src/server.ts",
    "postinstall": "git config core.hooksPath ./.githooks",
    "migration:create": "typeorm migration:create --dir src/migrations --name",
    "migration:generate": "npm run migration:run && ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:generate --name",
    "migration:revert": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:run",
    "eslint:fix": "eslint --fix '**/*.{t,j}s'",
    "eslint:check": "eslint '**/*.{t,j}s'",
    "postgres:start": "docker run -d -p 5432:5432 --name postgres -e POSTGRES_HOST_AUTH_METHOD=trust postgres:13-alpine",
    "database:create": "docker exec postgres psql -U postgres postgres -c \"CREATE DATABASE $(grep DB_DATABASE .env | cut -d= -f 2)\"",
    "test": "node --async-stack-traces node_modules/jest/bin/jest.js -w4"
  },
  "dependencies": {
    "@cubos/env": "^1.0.1",
    "@cubos/inject": "^1.2.0",
    "@sentry/node": "^6.2.2",
    "ajv": "^7.2.1",
    "ajv-formats": "^2.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.24.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.31",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@cubos/eslint-config": "^1.0.480297",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^14.14.34",
    "as-typed": "^1.3.0",
    "dotenv": "^8.2.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
